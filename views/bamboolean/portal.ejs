	<ol class="breadcrumb">
		<li class="breadcrumb-item"><a href="/">Bambank</a></li>
		<li class="breadcrumb-item active">Bamboolean online banking</li>
	</ol>

	<% if(flash && flash.warning){ %>
		<div class="alert alert-warning">
			<%= flash.warning %>
