	<% }else if(flash && flash.info){ %>
		<div class="alert alert-info">
			<%= flash.info %>
		</div>
	<% } %>

	<div class="" id="Portal">
		
	</div>

<% if (sails.config.environment === 'development') { %>
		<!-- <script src="<%=// sails.config.url %>:8080/dist/login.bundle.js"></script> -->
	<script src="http://localhost:8080/dist/portal.bundle.js"></script>

<% } else { %>
